{
  "name": "Find Text",
  "version": "1.0",
  "description": "Find all instances of a word(s) and highlight them on the page. Also display a found / not found banner",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["localhost:3000"],
      "js": ["header.js"],
      "css": ["header.css"]
    }
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "web_accessible_resources": ["header.html"],
  "manifest_version": 2
}
